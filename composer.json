{
    "name": "elao/enum",
    "description": "Enumerations for PHP and frameworks integrations",
    "keywords": ["enum", "symfony", "doctrine"],
    "type": "library",
    "homepage": "https://github.com/Elao/PhpEnums",
    "license": "MIT",
    "authors": [
        {
            "name": "Elao",
            "homepage": "http://www.elao.com"
        },
        {
            "name": "Maxime Steinhausser",
            "email": "maxime.steinhausser@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Elao\\Enum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Elao\\Enum\\Tests\\": "tests/" }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.2",
        "doctrine/dbal": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "friendsofphp/php-cs-fixer": "2.x@dev",
        "phpunit/phpunit": "^5.6",
        "symfony/browser-kit": "^3.1",
        "symfony/css-selector": "^3.1",
        "symfony/dependency-injection": "^3.1",
        "symfony/doctrine-bridge": "^3.1",
        "symfony/form": "^3.1",
        "symfony/framework-bundle": "^3.1",
        "symfony/phpunit-bridge": "^3.1",
        "symfony/serializer": "^3.1",
        "symfony/twig-bundle": "^3.1",
        "symfony/validator": "^3.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
